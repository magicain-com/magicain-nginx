{
  "dashboard": {
    "id": null,
    "title": "AI Agents Monitoring",
    "tags": ["magicain", "ai", "agents"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active AI Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "ai_active_sessions",
            "legendFormat": "Active Sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "AI Requests per Minute",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ai_requests_total[1m]) * 60",
            "legendFormat": "Requests/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Token Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(ai_tokens_consumed_total)",
            "legendFormat": "Total Tokens"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "AI Cost (USD)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(ai_cost_usd_total)",
            "legendFormat": "Total Cost"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "AI Response Time by Agent",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ai_response_time_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - 95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(ai_response_time_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - 50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 6,
        "title": "AI Requests by Agent",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_requests_total[5m])",
            "legendFormat": "{{agent}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per second",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}
      },
      {
        "id": 7,
        "title": "Token Usage by Agent",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_tokens_consumed_total[5m])",
            "legendFormat": "{{agent}} - {{type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Tokens per second",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14}
      },
      {
        "id": 8,
        "title": "AI Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_requests_total{status=\"error\"}[5m])",
            "legendFormat": "{{agent}} - errors"
          }
        ],
        "yAxes": [
          {
            "label": "Errors per second",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}